<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="joaopauloroslindo.calculadoradearea">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PedeDadoCirculo"
            android:screenOrientation="portrait"/>
        <activity android:name=".PedeDadoQuadrado"
            android:screenOrientation="portrait"/>
        <activity android:name=".PedeDadoTriangulo"
            android:screenOrientation="portrait"/>
        <activity android:name=".RespostaCirculo"
            android:screenOrientation="portrait"/>
        <activity android:name=".RespostaQuadrado"
            android:screenOrientation="portrait"/>
        <activity android:name=".RespostaTriangulo"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>